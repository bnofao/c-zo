{
  "name": "@czo/kit",
  "type": "module",
  "version": "0.0.1",
  "description": "Toolkit for authoring modules and interacting with Nuxt",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bnofao/czo.git",
    "directory": "packages/kit"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.mjs"
    },
    "./modules": {
      "import": "./dist/modules/index.mjs",
      "require": "./dist/modules/index.mjs"
    },
    "./graphql": {
      "import": "./dist/graphql/index.mjs",
      "require": "./dist/graphql/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": {
    "czo": "./dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "NODE_ENV=development unbuild --watch",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@adonisjs/fold": "catalog:common",
    "citty": "catalog:common",
    "consola": "catalog:common",
    "exsolve": "catalog:common",
    "pathe": "catalog:common",
    "jiti": "catalog:common",
    "tinyglobby": "catalog:common"
  },
  "devDependencies": {
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "graphql": "catalog:common",
    "hookable": "catalog:dev",
    "nitro": "catalog:dev",
    "unimport": "catalog:dev"
  }
}
