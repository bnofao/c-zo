{
  "name": "@czo/kit",
  "type": "module",
  "version": "0.0.1",
  "description": "Toolkit for authoring modules and interacting with Nuxt",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bnofao/czo.git",
    "directory": "packages/kit"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/index.mjs"
    },
    "./module": {
      "types": "./src/module/index.ts",
      "default": "./dist/module/index.mjs"
    },
    "./graphql": {
      "types": "./src/graphql/index.ts",
      "default": "./dist/graphql/index.mjs"
    },
    "./db": {
      "types": "./src/db/index.ts",
      "default": "./dist/db/index.mjs"
    },
    "./db/repository": {
      "types": "./src/db/repository/index.ts",
      "default": "./dist/db/repository/index.mjs"
    },
    "./cache": {
      "types": "./src/cache/index.ts",
      "default": "./dist/cache/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./src/index.ts",
  "bin": {
    "czo": "./dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "unbuild",
    "_dev": "NODE_ENV=development unbuild",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@adonisjs/fold": "catalog:common",
    "citty": "catalog:common",
    "consola": "catalog:common",
    "drizzle-orm": "catalog:common",
    "exsolve": "catalog:common",
    "jiti": "catalog:common",
    "kysely": "catalog:common",
    "pathe": "catalog:common",
    "pg": "catalog:common",
    "tinyglobby": "catalog:common"
  },
  "devDependencies": {
    "@types/pg": "catalog:types",
    "@vitest/coverage-v8": "catalog:testing",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "graphql": "catalog:common",
    "hookable": "catalog:dev",
    "nitro": "catalog:dev",
    "unimport": "catalog:dev",
    "vitest": "catalog:testing"
  }
}
