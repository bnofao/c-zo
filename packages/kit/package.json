{
  "name": "@czo/kit",
  "type": "module",
  "version": "0.0.1",
  "description": "Toolkit for authoring modules and interacting with Nuxt",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bnofao/czo.git",
    "directory": "packages/kit"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/index.mjs"
    },
    "./module": {
      "types": "./src/module/index.ts",
      "default": "./dist/module/index.mjs"
    },
    "./graphql": {
      "types": "./src/graphql/index.ts",
      "default": "./dist/graphql/index.mjs"
    },
    "./db": {
      "types": "./src/db/index.ts",
      "default": "./dist/db/index.mjs"
    },
    "./cache": {
      "types": "./src/cache/index.ts",
      "default": "./dist/cache/index.mjs"
    },
    "./events": {
      "types": "./src/events/index.ts",
      "default": "./dist/events/index.mjs"
    },
    "./queue": {
      "types": "./src/queue/index.ts",
      "default": "./dist/queue/index.mjs"
    },
    "./config": {
      "types": "./src/config.ts",
      "default": "./dist/config.mjs"
    },
    "./event-bus": {
      "types": "./src/event-bus/index.ts",
      "default": "./dist/event-bus/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./src/index.ts",
  "bin": {
    "czo": "./dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "unbuild",
    "_dev": "NODE_ENV=development unbuild",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "amqplib": "^0.10.0",
    "bullmq": "^5.0.0",
    "ioredis": "^5.0.0",
    "nitro": "^2.0.0 || ^3.0.0"
  },
  "peerDependenciesMeta": {
    "amqplib": {
      "optional": true
    },
    "bullmq": {
      "optional": true
    },
    "ioredis": {
      "optional": true
    },
    "nitro": {
      "optional": true
    }
  },
  "dependencies": {
    "@adonisjs/fold": "catalog:common",
    "change-case": "catalog:common",
    "citty": "catalog:common",
    "consola": "catalog:common",
    "drizzle-orm": "catalog:common",
    "exsolve": "catalog:common",
    "hookable": "catalog:dev",
    "jiti": "catalog:common",
    "kysely": "catalog:common",
    "pathe": "catalog:common",
    "pg": "catalog:common",
    "tinyglobby": "catalog:common",
    "zod": "catalog:common"
  },
  "devDependencies": {
    "@types/amqplib": "catalog:types",
    "@types/pg": "catalog:types",
    "@vitest/coverage-v8": "catalog:testing",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "graphql": "catalog:common",
    "nitro": "catalog:dev",
    "unimport": "catalog:dev",
    "vitest": "catalog:testing"
  }
}
