{
  "name": "@czo/product",
  "type": "module",
  "version": "0.0.1",
  "description": "Product module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/czo/czo.git",
    "directory": "packages/modules/product"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.mjs"
    },
    "./utils": {
      "import": "./dist/utils/index.mjs",
      "require": "./dist/utils/index.mjs"
    },
    "./database": {
      "import": "./dist/database/index.mjs",
      "require": "./dist/database/index.mjs"
    },
    "./services": {
      "import": "./dist/services/index.mjs",
      "require": "./dist/services/index.mjs"
    },
    "./validators": {
      "import": "./dist/validators/index.mjs",
      "require": "./dist/validators/index.mjs"
    },
    "./schema/*": {
      "import": "./dist/schema/*.js",
      "require": "./dist/schema/*.js"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "unbuild",
    "_dev": "NODE_ENV=development unbuild",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "generate": "graphql-codegen",
    "migrate:create": "kysely migration:create",
    "migrate:latest": "kysely migrate:latest",
    "migrate:up": "kysely migrate:up",
    "migrate:down": "kysely migrate:down",
    "migrate:status": "kysely migrate:status",
    "generate:types": "kysely-codegen --dialect postgres --out-file src/database/types.ts",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "@czo/kit": "workspace:*",
    "drizzle-orm": "catalog:common",
    "kysely": "catalog:common",
    "nitro": "catalog:dev",
    "pg": "catalog:common"
  },
  "peerDependenciesMeta": {
    "drizzle-orm": {
      "optional": true
    },
    "kysely": {
      "optional": true
    },
    "pg": {
      "optional": true
    }
  },
  "dependencies": {
    "@adonisjs/fold": "catalog:common",
    "ulid": "catalog:common",
    "zod": "catalog:common"
  },
  "devDependencies": {
    "@czo/kit": "workspace:*",
    "@types/pg": "catalog:types",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "drizzle-orm": "catalog:common",
    "graphql": "catalog:common",
    "kysely": "catalog:common",
    "kysely-codegen": "catalog:dev",
    "kysely-ctl": "catalog:dev",
    "nitro": "catalog:dev",
    "pg": "catalog:common",
    "testcontainers": "catalog:dev"
  },
  "kysely-codegen": {
    "camelCase": true
  }
}
